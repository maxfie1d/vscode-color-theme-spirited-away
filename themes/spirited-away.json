{
    "name": "Spirited Away",
    "tokenColors": [
        {
            "settings": {
                "background": "$$defaultBackground",
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "$green"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "constant.character.escape.ts"
            ],
            "settings": {
                "foreground": "$lightOrange"
            }
        },
        {
            "name": "Regex",
            "scope": [
                "constant.other.character-class.regexp",
                "constant.other.character-class.set.regexp",
                "constant.character.escape.backslash.regexp"
            ],
            "settings": {
                "foreground": "$lightOrange"
            }
        },
        {
            "name": "Template expression. e.g. ` '${' '}' `",
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "foreground": "$lightPink"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "$yellowGreen"
            }
        },
        {
            "name": "Built-in constant. e.g. 'undefined', 'true', 'false'",
            "scope": "constant.language",
            "settings": {
                "foreground": "$blue"
            }
        },
        {
            "name": "'bold', 'italic', 'auto' (CSS)",
            "scope": [
                "support.constant.property-value"
            ],
            "settings": {
                "foreground": "$lightOrange"
            }
        },
        {
            "name": "Colors (CSS)",
            "scope": [
                "constant.other.color.rgb-value.hex",
                "support.constant.color.w3c-standard-color-name",
                "support.constant.color.w3c-extended-color-name"
            ],
            "settings": {
                "foreground": "$lightOrange"
            }
        },
        {
            "name": "Pseudo class, element (CSS)",
            "scope": [
                "entity.other.attribute-name.pseudo-element",
                "entity.other.attribute-name.pseudo-class"
            ],
            "settings": {
                "foreground": "$purple"
            }
        },
        {
            "name": "@import, @extend, @mixin (SCSS)",
            "scope": [
                "keyword.control.at-rule"
            ],
            "settings": {
                "foreground": "$purple"
            }
        },
        {
            "name": "Keyword. e.g. 'export', 'return'",
            "scope": "keyword",
            "settings": {
                "foreground": "$lightPink"
            }
        },
        {
            "name": "Storage. e.g. '=', '<<'",
            "scope": "storage",
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "Storage modifier. e.g. 'public'",
            "scope": "storage.modifier",
            "settings": {
                "foreground": "$blue"
            }
        },
        {
            "name": "Imported items",
            "scope": "storage.modifier.import",
            "settings": {
                "foreground": "$lightPink"
            }
        },
        {
            "name": "'(' ')' ';' '{' '}' '[' ']' '<' '>' '::'",
            "scope": [
                "punctuation.definition.method-parameters",
                "punctuation.definition.seperator.parameter",
                "punctuation.definition.parameters",
                "punctuation.definition.block",
                "punctuation.definition.typeparameters",
                "punctuation.separator.comma",
                "punctuation.terminator",
                "punctuation.section.method",
                "meta.method.identifier",
                "meta.brace.round",
                "meta.brace.square",
                "punctuation.definition.entity"
            ],
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "':' switch-case",
            "scope": "punctuation.definition.section.case-statement",
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "'.' method accessor",
            "scope": "punctuation.accessor",
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "'this' 'super'",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "$blue"
            }
        },
        {
            "name": "Storage type. e.g. 'function', 'let', 'const'",
            "scope": "storage.type",
            "settings": {
                "foreground": "$blue"
            }
        },
        {
            "name": "Arrow",
            "scope": "storage.type.function.arrow",
            "settings": {
                "foreground": "$lightPink"
            }
        },
        {
            "name": "Class name. e.g. 'Map', Action<'T'>",
            "scope": [
                "entity.name.class",
                "entity.name.type.class",
                "entity.other.inherited-class",
                "storage.type.generic",
                "storage.class"
            ],
            "settings": {
                "foreground": "$lightPink"
            }
        },
        {
            "name": "Entity name type",
            "scope": "entity.name.type",
            "settings": {
                "foreground": "$lightPink"
            }
        },
        {
            "name": "Function and method name",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.method",
                "meta.function-call.generic"
            ],
            "settings": {
                "foreground": "$orange"
            }
        },
        {
            "name": "Method call and body",
            "scope": [
                "meta.method-call",
                "meta.method.body"
            ],
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "Tag definition '< />'",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "$trivialGray"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "$lightPink"
            }
        },
        {
            "name": "Attribute name",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "DOCTYPE (HTML)",
            "scope": "meta.tag.sgml.doctype.html",
            "settings": {
                "foreground": "$blue"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "variable.other.readwrite",
                "meta.definition.variable.name"
            ],
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "Variable (SCSS)",
            "scope": "variable.scss",
            "settings": {
                "foreground": "$lightBlue"
            }
        },
        {
            "name": "Property",
            "scope": [
                "support.variable.property",
                "variable.property",
                "variable.object.property",
                "meta.definition.property",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "Imported item",
            "scope": [
                "variable.other.readwrite.alias"
            ],
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "Enum",
            "scope": [
                "variable.other.enummember"
            ],
            "settings": {
                "foreground": "$yellowGreen"
            }
        },
        {
            "name": "Library class/type. e.g. 'string', 'number'",
            "scope": [
                "support.type",
                "support.class",
                "storage.type.core"
            ],
            "settings": {
                "foreground": "$lightPink"
            }
        },
        {
            "name": "Decoratos ('@'Component)",
            "scope": [
                "punctuation.decorator"
            ],
            "settings": {
                "foreground": "$purple"
            }
        },
        {
            "name": "'@'param (jsdoc)",
            "scope": [
                "punctuation.definition.block.tag"
            ],
            "settings": {
                "foreground": "$blue"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "e.g. name':' string, string '|' number",
            "scope": "keyword.operator.type",
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "'new', 'of', and other keywords",
            "scope": [
                "keyword.operator.new",
                "keyword.control.new",
                "keyword.operator.expression",
                "keyword.other"
            ],
            "settings": {
                "foreground": "$blue"
            }
        },
        {
            "name": "Dereference operator ('.')",
            "scope": "keyword.operator.dereference",
            "settings": {
                "foreground": "$$defaultForeground"
            }
        },
        {
            "name": "Markdown header",
            "scope": "markup.heading",
            "settings": {
                "foreground": "$lightPink"
            }
        },
        {
            "name": "Markdown bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "$blue"
            }
        },
        {
            "name": "Markdown italic",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic",
                "foreground": "$blue"
            }
        },
        {
            "name": "Markdown list",
            "scope": "beginning.punctuation.definition.list",
            "settings": {
                "foreground": "$lightPink"
            }
        },
        {
            "name": "Markdown link",
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "$lightBlue"
            }
        },
        {
            "name": "Markdown inline code",
            "scope": [
                "markup.inline.raw",
                "punctuation.definition.raw"
            ],
            "settings": {
                "foreground": "$blue"
            }
        },
        {
            "name": "Markdown code block",
            "scope": [
                "punctuation.definition.markdown",
                "fenced_code.block.language"
            ],
            "settings": {
                "foreground": "$orange"
            }
        },
        {
            "name": "Markdown quote",
            "scope": "beginning.punctuation.definition.quote",
            "settings": {
                "foreground": "$green"
            }
        }
    ],
