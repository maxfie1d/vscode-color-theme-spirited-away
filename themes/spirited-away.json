    },
    "colorPalette": {
        // Color
        "$gray": "#383838",
        "$trivialGray": "#777",
        "$dark": "#151515",
        "$lightDark": "#222",
        "$lightGray": "#CFCFCF",
        "$green": "#6d966c",
        "$orange": "#da846b",
        "$lightOrange": "#e7ab65",
        "$yellowGreen": "#AFC563",
        "$blue": "#268ac3",
        "$lightBlue": "#5cafdf",
        "$pink": "#EC5151",
        "$lightPink": "#ef6363",
        "$red": "#BD3538",
        "$purple": "#9977a2",
        "$white": "#FFF",
        "$black": "#000",
        // Alpha
        "@1B": "1B",
        "@20": "20",
        "@28": "28",
        "@30": "30",
        "@40": "40",
        "@50": "50",
        "@60": "60",
        "@mid": "80",
        "@B0": "B0"
    },
    "alias": {
        "$$border": "$gray",
        "$$defaultBackground": "$dark",
        "$$defaultForeground": "$lightGray"
    }
}